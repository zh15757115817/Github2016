.news_one
	width 92%
	height 120px
	background #fff
	margin 0 auto
	margin-top 10px
	border-radius 4px
	box-shadow 0px 0px 3px 1px #d0d0d0
.one_header
	display block
	width 100%
	display flex
	font-size 14px
	line-height 40px
	border-bottom 1px solid #dcdcdc
	
	.one_title
		margin 0 auto
		font-size 16px
		flex 1
		text-align left
		height 40px
		color #1
		text-indent 1em
	.one_date
		flex 1
		font-size 12px
		margin 0 auto
		text-align right
		height 40px
		color grey
		padding-right 1em
		
.one_text
	display block
	width 94%
	line-height 16px
	font-size 12px
	text-indent 5px
	margin 0 auto
	margin-top 10px
	color grey
