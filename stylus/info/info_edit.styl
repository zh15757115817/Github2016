body
	background #EFEFF4

.head
	width 100%
	height 50px
	background #fff url(../../img/nav_back_icon@2x.png) no-repeat 15px 
	background-size 2.5%
	line-height 50px
	text-align center
	margin-bottom 10px
.link_all
	
	width 100%
	
	line-height 35px
	font-size 12px
	background #fff
.link_one
	display flex
	border-bottom 1px solid #dcdcdc
	margin-left 15px
	.link_txt
		flex 3

	.link_cell
		flex 2
		
	.link_txt_img
		height 40px
		width 40px
		flex 3
		background url(../../img/login_user-head@2x.png) no-repeat left
		background-size 15%
		
	.link_more
		display block
		float right
		width 20px
		height 40px
		background url(../../img/nav_icon_back@2x.png) no-repeat center
		background-size 40%

.optional
	width 100%
	margin 0 auto
	background #fff

	.opt_more
		width 100%
		display flex

		.more-left
			flex 1
			margin-bottom 15px
			margin-top 10px
		.left
			height 60px
			margin 10px 15px
			border-radius 3px
			background grey
		.txt
			display block
			text-align center
			font-size 12px
			color grey


	



	
